sum = 0
for i in range(1,6):
	a= float(input(f"subject {i}: "))
	sum = sum + a
	avg = sum/5
print("Average Marks: %.2f"%avg)
if(avg>= 90 and avg<= 100):
	print("Grade: A\n")
elif(avg>= 80 and avg<= 89):
	print("Grade: B\n")
elif(avg>= 70 and avg<= 79):
	print("Grade: C\n")
elif(avg>= 60 and avg <= 69):
	print("Grade: D\n")
else:
	print("Grade: F\n")
